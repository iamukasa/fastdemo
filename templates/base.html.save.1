<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <title>Plagarise.io</title>
    <link rel="stylesheet" href="{{url_for('static',filename='css/main.css')}}"/>

    <meta name="viewport" content="width=device-width, initial-scale=1">
<meta http-equiv="Content-Type" content="text/html; charset=utf-8" />
<meta name="keywords" content="plagarise,ai,rnn,lstm,artificial intelligence" />
<script type="application/x-javascript"> addEventListener("load", function() { setTimeout(hideURLbar, 0); }, false);
		function hideURLbar(){ window.scrollTo(0,1); } </script>
    <link href="{{url_for('static',filename='css/bootstrap.css')}}" rel="stylesheet" type="text/css" media="all" />
<link href="{{url_for('static',filename='css/font-awesome.css')}}" rel="stylesheet">
<link rel="stylesheet" href="{{url_for('static',filename='css/lightbox.css')}}" type="text/css" media="all" /><!--gallery-popup-css-->
<link href="{{url_for('static',filename='css/popuo-box.css')}}" rel="stylesheet" type="text/css" media="all" /><!-- Pop-up -->
<link href="{{url_for('static',filename='css/style.css')}}" rel="stylesheet" type="text/css" media="all" />
<!--fonts-->
<link href="//fonts.googleapis.com/css?family=Josefin+Sans:100,300,400,600,700" rel="stylesheet">
<link href="//fonts.googleapis.com/css?family=Lato:300,400,700,900" rel="stylesheet">
<!--//fonts-->
</head>
<body id="home">

<div class="banner">
    <div class="header">
        <div class="w3_agile_logo">
            <h1><a href="base.html">Plagarise.io:creating new literature</a></h1>
        </div>
        <div class="clearfix"> </div>
        <div class="agile_banner_info">
			<h2><span>Welcome to Plagarise.io </h2>
				<div id="typed-strings" class="agileits_w3layouts_strings">
						<p><i>copy it</i></p>
						<p><i>paste it </i></p>
						<p><i>train it</i></p>
						<p><i>with the help of ai</i></p>
					</div>
				<span id="typed" style="white-space:pre;"></span>
			</div>
        </div>



    </div>

    <form action="/generate" method="post" id="entering">
    <p>Generate text by entering text you want to learn from</p>
    <div class="form-group mine">
    <textarea class="form-control" type="text" id="entertext" name="entertext" placeholder="Text to learn from">
    </textarea>
        <button type="submit" href="/generate" class="btn btn-primary" id="generate" name="generate">Generate</button>
     </div>
    <!--  <p>Or select a .txt file from your device to analyse(this may take a while)</p>
    <div class="form-group mine">
    <input class="form-control" type="file" id="enterfile" name="enterfile" placeholder="Text to learn from">
        <button type="submit" href="#small_dialog" class="btn btn-primary" id="generatefile" name="generatefile">Generate</button>
     </div>
 -->



    </form>
</div>

<div class="progress">
    <div class="progress-bar progress-bar-striped active">

    </div>
</div>

<div id="small-dialog"
     class="mfp-hide w3ls_small_dialog wthree_pop">
    <div class="agileits_modal_body">
        <h1>Churing the data</h1>
    </div>
</div>

<form action="/summarise" method="post" id="enteringsum">
    <p>Summarise large body of text</p>
    <div class="form-group mine">
        <textarea class="form-control" type="text" id="entertextsum" name="entertextsum" placeholder="Text to learn from"></textarea>
        <button type="submit" href="/generate" class="btn btn-primary" id="generatesum" name="generate">Summarise</button>
     </div>
{% block content%}

{% endblock %}





<!-- js -->


<script type="text/javascript" src="{{url_for('static',filename='js/jquery-2.1.4.min.js')}}"></script>


<!-- Navigation-JavaScript -->
<!-- menu -->
<script>
	$(document).ready(function () {
	  $('.mobile-nav-button').on('click', function() {
	  $( ".mobile-nav-button .mobile-nav-button__line:nth-of-type(1)" ).toggleClass( "mobile-nav-button__line--1");
	  $( ".mobile-nav-button .mobile-nav-button__line:nth-of-type(2)" ).toggleClass( "mobile-nav-button__line--2");
	  $( ".mobile-nav-button .mobile-nav-button__line:nth-of-type(3)" ).toggleClass( "mobile-nav-button__line--3");

	  $('.mobile-menu').toggleClass('mobile-menu--open');
	  return false;
	});
	});
</script>
<!-- //menu -->
<!-- Gallery-plugin -->

<script src="{{url_for('static',filename='js/jquery.mobile.custom.min.js')}}"></script>
<script src="{{url_for('static',filename='js/jquery.cm-overlay.js')}}"></script>
		<script>
			$(document).ready(function(){
				$('.cm-overlay').cmOverlay();
			});
		</script>
<!-- //Gallery-plugin -->
<!-- responsiveslides -->

<script src="{{url_for('static',filename='js/responsiveslides.min.js')}}"></script>
			<script>
									// You can also use "$(window).load(function() {"
									$(function () {
									 // Slideshow 4
									$("#slider3").responsiveSlides({
										auto: true,
										pager: false,
										nav: true,
										speed: 500,
										namespace: "callbacks",
										before: function () {
									$('.events').append("<li>before event fired.</li>");
									},
									after: function () {
										$('.events').append("<li>after event fired.</li>");
										}
										});
										});
			</script>
<!-- //responsiveslides -->

	<!-- //Navigation-JavaScript -->
<!--pop-up-box -->


					<script src="{{url_for('static',filename='js/jquery.magnific-popup.js')}}" type="text/javascript"></script>
					<script>
					$(document).ready(function() {
					$('.popup-with-zoom-anim').magnificPopup({
					type: 'inline',
					fixedContentPos: false,
					fixedBgPos: true,
					overflowY: 'auto',
					closeBtnInside: true,
					preloader: false,
					midClick: true,
					removalDelay: 300,
					mainClass: 'my-mfp-zoom-in'
					});

					});
					</script>
<!-- //pop-up-box -->

<!-- banner-type-text -->

	<script src="{{url_for('static',filename='js/typed.js')}}" type="text/javascript"></script>
    <script>
		$(function(){

			$("#typed").typed({
				// strings: ["Typed.js is a <strong>jQuery</strong> plugin.", "It <em>types</em> out sentences.", "And then deletes them.", "Try it out!"],
				stringsElement: $('#typed-strings'),
				typeSpeed: 30,
				backDelay: 500,
				loop: false,
				contentType: 'html', // or text
				// defaults to false for infinite loop
				loopCount: false,
				callback: function(){ foo(); },
				resetCallback: function() { newTyped(); }
			});

			$(".reset").click(function(){
				$("#typed").typed('reset');
			});

		});

		function newTyped(){ /* A new typed object */ }

		function foo(){ console.log("Callback"); }
    </script>
<!-- //banner-type-text -->
<!-- jarallax-js -->


			<script src="{{url_for('static',filename='js/SmoothScroll.min.js')}}"></script>
<!-- start-smoth-scrolling -->
<script type="text/javascript" src="{{url_for('static',filename='js/js/move-top.js')}}"></script>
<script type="text/javascript" src="{{url_for('static',filename='js/easing.js')}}"></script>
<script type="text/javascript">
	jQuery(document).ready(function($) {
		$(".scroll").click(function(event){
			event.preventDefault();
			$('html,body').animate({scrollTop:$(this.hash).offset().top},1000);
		});
	});
</script>
<!-- start-smoth-scrolling -->
	<!-- smooth scrolling -->
	<script type="text/javascript">
		$(document).ready(function() {
		/*
			var defaults = {
			containerID: 'toTop', // fading element id
			containerHoverID: 'toTopHover', // fading element hover id
			scrollSpeed: 1200,
			easingType: 'linear'
			};
		*/
		$().UItoTop({ easingType: 'easeOutQuart' });
		});
	</script>

	<div class="arr-w3ls">
	<a href="#home" id="toTop" style="display: block;"> <span id="toTopHover" style="opacity: 1;"> </span></a>
	</div>
<!-- //smooth scrolling -->


<script type="text/javascript" src="{{url_for('static',filename='js/bootstrap-3.1.1.min.js')}}"></script>

</body>
